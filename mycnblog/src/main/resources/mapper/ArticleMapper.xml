<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiehfut.mycnblog.mapper.ArticleMapper">

    <select id="getMyList" resultType="com.jiehfut.mycnblog.model.ArticleInfo">
        select *
        from articleinfo
        where uid = #{uid};
    </select>

</mapper>